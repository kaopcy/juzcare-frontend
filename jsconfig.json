{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/graphql/*": ["src/graphql/*"],
            "@/routes/*": ["src/routes/*"],
            "@/redux/*": ["src/redux/*"],
            "@/slices/*": ["src/redux/slices/*"],
            "@/hooks/*": ["src/hooks/*"],
            "@/components/*": ["src/components/*"],
            "@/layouts/*": ["src/layouts/*"],
            "@/configs/*": ["src/configs/*"],
            "@/theme/*": ["src/theme/*"],
            "@/guards/*": ["src/guards/*"],
            "@/sections/*": ["src/sections/*"],
            "@/pages/*": ["src/pages/*"],
            "@/contexts/*": ["src/contexts/*"],
            "@/utils/*": ["src/utils/*"]
        }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}
